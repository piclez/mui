<%= mui_grid(:columns => 2) do %>
  <%= mui_cell(:width => '25%') do %>
    <%= mui_block do %>
      <% @photosets.each do |photoset| %>
        <%= mui_flickr_tab(:photoset => photoset) %>
      <% end %>
    <% end =%>
  <% end =%>
  <%= mui_cell(:width => '75%') do %>
    <%= mui_block do %>
      <% @photos.each do |photo| %>
        <%= mui_flickr_button(:photo => photo) %>
      <% end %>
    <% end =%>
  <% end =%>
<% end =%>
