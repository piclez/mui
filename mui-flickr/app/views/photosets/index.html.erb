<%= mui_flickr do %>
  <%= mui_grid(:type => 'desktop') do %>
    <%= mui_row do %>
      <%= mui_column(:width => '25%') do %>
        <% @photosets.each do |photoset| %>
          <%= mui_flickr_tab(:photoset => photoset) %>
        <% end %>
      <% end =%>
      <%= mui_column do %>
        <% @photos.each do |photo| %>
          <%= mui_flickr_button(:photo => photo) %>
        <% end %>
      <% end =%>
    <% end =%>
  <% end =%>
<% end =%>
