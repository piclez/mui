$(document).ready(function(){
  $('.mi_click_dialog_open').click(function(){
  	$('.mi_dialog_target').hide().load(this.id).fadeIn();
  	return false;
  });
  $('.mi_click').click(function(){
  	location.href=this.id;
  	return false;
  });
  $('.mi_focus').focus();
});
$(document).keyup(function(event){
  if (event.keyCode == 112) {
	  $('.mi_dialog_target').hide().load('/password/read').fadeIn();
  	return false;
  }
});