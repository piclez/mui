$(document).keyup(function(event){
  if (event.keyCode == 112) {
    <% if mui_password? %>
      window.location = '<%= url(:mui_password_exit) %>';
    <% else %>
      $('.mui_window_target').fadeOut(function(){
        $('.mui_window_target').empty().hide().load('<%= url(:mui_password_read) %>').fadeIn();
      });
    <% end %>
  	return false;
  }
});
