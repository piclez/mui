<%= mui_window(:title => 'Create Password') do %>
  <%= mui_form(:action => url(:mui_password_create), :model => @password) do %>
    <%= mui_grid(:type => 'form') do %>
      <%= partial(:form) %>
      <%= mui_row do %>
        <%= mui_column do %>
          <%= mui_button(:title => 'Create', :submit => true, :tone => 'positive')%>
        <% end =%>
      <% end =%>
    <% end =%>
  <% end =%>
<% end =%>
