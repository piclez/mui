<%= mui_window(:label => 'Password Entry') do %>
  <%= mui_form(:action => url(:mui_password_read), :model => @password) do %>
    <%= mui_form_password(:password, :focus => true, :inline => true, :label => 'Password') %>
    <%= mui_button(:label => 'Enter', :submit => true, :tone => 'positive')%>
  <% end =%>
<% end =%>